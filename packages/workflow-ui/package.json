{
  "name": "@genfeedai/core-ui",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "description": "Shared UI components for Genfeed workflow editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genfeedai/core.git",
    "directory": "packages/workflow-ui"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./canvas": {
      "types": "./dist/canvas.d.ts",
      "import": "./dist/canvas.js",
      "require": "./dist/canvas.cjs"
    },
    "./nodes": {
      "types": "./dist/nodes.d.ts",
      "import": "./dist/nodes.js",
      "require": "./dist/nodes.cjs"
    },
    "./panels": {
      "types": "./dist/panels.d.ts",
      "import": "./dist/panels.js",
      "require": "./dist/panels.cjs"
    },
    "./toolbar": {
      "types": "./dist/toolbar.d.ts",
      "import": "./dist/toolbar.js",
      "require": "./dist/toolbar.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js",
      "require": "./dist/hooks.cjs"
    },
    "./stores": {
      "types": "./dist/stores.d.ts",
      "import": "./dist/stores.js",
      "require": "./dist/stores.cjs"
    },
    "./provider": {
      "types": "./dist/provider.d.ts",
      "import": "./dist/provider.js",
      "require": "./dist/provider.cjs"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.js",
      "require": "./dist/ui.cjs"
    },
    "./lib": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs"
    },
    "./styles": "./src/styles/workflow-ui.css"
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "@xyflow/react": ">=12.0.0",
    "next": ">=14.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "zustand": ">=5.0.0",
    "zundo": ">=2.0.0"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@genfeedai/types": "workspace:*",
    "@xyflow/react": "12.10.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "tsup": "8.4.0",
    "typescript": "5.9.3",
    "zustand": "5.0.3",
    "zundo": "2.3.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "bun run build"
  }
}
