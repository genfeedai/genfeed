{
  "name": "@genfeedai/core",
  "version": "0.1.1",
  "license": "AGPL-3.0",
  "description": "Core utilities for Genfeed workflow engine",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genfeedai/core.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./topological-sort": {
      "types": "./dist/topological-sort.d.ts",
      "import": "./dist/topological-sort.js",
      "require": "./dist/topological-sort.cjs"
    },
    "./validation": {
      "types": "./dist/validation.d.ts",
      "import": "./dist/validation.js",
      "require": "./dist/validation.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "bun run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@genfeedai/types": "workspace:*"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "3.0.4",
    "tsup": "8.4.0",
    "typescript": "5.9.3",
    "vitest": "3.0.4"
  }
}
